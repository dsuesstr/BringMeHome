{
    "name": "BringMeHome",
    "version": "0.0.1",
    "description": "The Bring me Home application looks up the fatest way to your home.",
    "main": "main.js",
    "dependencies": {
        "tsd": "*",
        "glob": "*",
        "gulp": "*",
        "gulp-concat": "*",
        "gulp-uglify": "*",
        "gulp-if": "*",
        "gulp-notify": "*",
        "gulp-minify-css": "*",
        "gulp-clean": "*",
        "gulp-size": "*",
        "jshint-stylish": "*",
        "gulp-cache": "*",
        "gulp-header": "*",
        "gulp-load-plugins": "*",
        "gulp-load-utils": "*",
        "chalk": "*",
        "dateformat": "*",
        "minimist": "*",
        "gulp-size": "*",
        "gulp-ng-annotate": "*",
        "gulp-angular-templatecache": "*"
    },
    "author": "2BIT GmbH",
    "webroot": "../",
    "webapp": "bringmehome",
    "dest": [ "../2Bit.BringMeHome.PhoneGap/www/" ],
    "paths": {
        "source": {
            "images": [
                "images/**/*"
            ],
            "js": [
                "app/injections.js",
                "app/app.js",
                "app/views/*.js",
                "app/constants.js",
                "app/routes.js",
                "app/directives/MaxHeight.js",
                "app/directives/PieMenu.js",
                "app/controllers/MenuController.js",
                "app/controllers/HomeController.js",
                "app/controllers/LocationController.js",
                "app/controllers/LanguageController.js",
                "app/controllers/ConnectionsController.js",
                "app/controllers/TravelDetailsController.js",
                "app/controllers/MapController.js",
                "app/filters/DurationFilter.js",
                "app/filters/FormatFilter.js",
                "app/interfaces/Framework.js",
                "app/interfaces/GeoPosition.js",
                "app/interfaces/Plugins.js",
                "app/interfaces/Routes.js",
                "app/interfaces/Services.js",
                "app/services/SelectedRoute.js",
                "app/services/Event.js",
                "app/services/GeoLocationProvider.js",
                "app/run-services/InternetConnectionWatcher.js",
                "app/services/LocalStorage.js",
                "app/run-services/LocalizationConfig.js",
                "app/services/AvailableRoutes.js",
                "app/services/Navigation.js",
                "app/services/Logger.js",
                "app/services/RoutesConverter.js",
                "app/services/OpenLink.js",
                "app/services/OpenPedestrianRoute.js",
                "app/services/SelectedLocation.js",
                "app/services/RoutesProvider.js",
                "app/services/SelectedPedestrianRoute.js",
                "app/services/TargetAddress.js",
                "app/services/Strings.js",
                "app/services/Urls.js",
                "app/services/AddressesProvider.js",
                "app/run-services/ClearHistory.js",
                "app/run-services/IonicConfig.js",                
                "app/interfaces/models/Models.js",
                "app/interfaces/models/messages/Responses.js",
                "app/registrations.js",
                "app/run-services/AngularInit.js"
            ],
            "css": [
                "bower_components/ionic/release/css/ionic.css",                
                "styles/toastr.css",
                "styles/pie.css",
                "styles/default.css"
            ]
        },
        "dest": {
            "css": "../2Bit.BringMeHome.PhoneGap/www/css/",
            "js": "../2Bit.BringMeHome.PhoneGap/www/scripts/app/",
            "images": "../2Bit.BringMeHome.PhoneGap/www/images/",
            "views": "./app/views/",
            "wp8": "../2Bit.BringMeHome.WP8/www/",
            "cordova": "../2Bit.BringMeHome.Cordova/"
        }
